<div class="line"><a name="l00045"></a><span class="lineno">   45</span>&#160;                *sp++ = *s;</div>
<div class="line"><a name="l00046"></a><span class="lineno">   46</span>&#160;                <span class="keywordflow">if</span> (*s == 0)</div>
<div class="line"><a name="l00047"></a><span class="lineno">   47</span>&#160;                        <span class="keywordflow">break</span>;</div>
