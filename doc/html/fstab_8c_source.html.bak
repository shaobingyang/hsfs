<div class="line"><a name="l00310"></a><span class="lineno">  310</span>&#160;        <span class="keywordtype">int</span> tries = 100000, i;</div>
<div class="line"><a name="l00311"></a><span class="lineno">  311</span>&#160;        <span class="keywordtype">char</span> linktargetfile[<a class="code" href="fstab_8c.html#a30e1275324a9627ce7868c7630e8cdcc">MOUNTLOCK_LINKTARGET_LTH</a>];</div>
<div class="line"><a name="l00312"></a><span class="lineno">  312</span>&#160;</div>
<div class="line"><a name="l00313"></a><span class="lineno">  313</span>&#160;        <a class="code" href="xcommon_8c.html#a115ce574fe5cca364afa17c11067cb66">at_die</a> = <a class="code" href="fstab_8h.html#a7ce6803a0833d2c6161b7abbb2d2b8dc">unlock_mtab</a>;</div>
<div class="line"><a name="l00314"></a><span class="lineno">  314</span>&#160;</div>
<div class="line"><a name="l00315"></a><span class="lineno">  315</span>&#160;        <span class="keywordflow">if</span> (!signals_have_been_setup) {</div>
<div class="line"><a name="l00316"></a><span class="lineno">  316</span>&#160;                <span class="keywordtype">int</span> sig = 0;</div>
<div class="line"><a name="l00317"></a><span class="lineno">  317</span>&#160;                <span class="keyword">struct </span>sigaction sa;</div>
<div class="line"><a name="l00318"></a><span class="lineno">  318</span>&#160;</div>
<div class="line"><a name="l00319"></a><span class="lineno">  319</span>&#160;                sa.sa_handler = handler;</div>
<div class="line"><a name="l00320"></a><span class="lineno">  320</span>&#160;                sa.sa_flags = 0;</div>
<div class="line"><a name="l00321"></a><span class="lineno">  321</span>&#160;                sigfillset (&amp;sa.sa_mask);</div>
<div class="line"><a name="l00322"></a><span class="lineno">  322</span>&#160;  </div>
<div class="line"><a name="l00323"></a><span class="lineno">  323</span>&#160;                <span class="keywordflow">while</span> (sigismember (&amp;sa.sa_mask, ++sig) != -1</div>
<div class="line"><a name="l00324"></a><span class="lineno">  324</span>&#160;                       &amp;&amp; sig != SIGCHLD) {</div>
<div class="line"><a name="l00325"></a><span class="lineno">  325</span>&#160;                        <span class="keywordflow">if</span> (sig == SIGALRM)</div>
<div class="line"><a name="l00326"></a><span class="lineno">  326</span>&#160;                                sa.sa_handler = setlkw_timeout;</div>
<div class="line"><a name="l00327"></a><span class="lineno">  327</span>&#160;                        <span class="keywordflow">else</span></div>
<div class="line"><a name="l00328"></a><span class="lineno">  328</span>&#160;                                sa.sa_handler = handler;</div>
<div class="line"><a name="l00329"></a><span class="lineno">  329</span>&#160;                        sigaction (sig, &amp;sa, (<span class="keyword">struct</span> sigaction *) 0);</div>
<div class="line"><a name="l00330"></a><span class="lineno">  330</span>&#160;                }</div>
<div class="line"><a name="l00331"></a><span class="lineno">  331</span>&#160;                signals_have_been_setup = 1;</div>
<div class="line"><a name="l00332"></a><span class="lineno">  332</span>&#160;        }</div>
<div class="line"><a name="l00333"></a><span class="lineno">  333</span>&#160;</div>
<div class="line"><a name="l00334"></a><span class="lineno">  334</span>&#160;        sprintf(linktargetfile, <a class="code" href="fstab_8c.html#a87eba1affb7fa5f53bde0ab5fbce5d54">MOUNTLOCK_LINKTARGET</a>, getpid ());</div>
<div class="line"><a name="l00335"></a><span class="lineno">  335</span>&#160;</div>
<div class="line"><a name="l00336"></a><span class="lineno">  336</span>&#160;        i = open (linktargetfile, O_WRONLY|O_CREAT, 0);</div>
<div class="line"><a name="l00337"></a><span class="lineno">  337</span>&#160;        <span class="keywordflow">if</span> (i &lt; 0) {</div>
<div class="line"><a name="l00338"></a><span class="lineno">  338</span>&#160;                <span class="keywordtype">int</span> errsv = errno;</div>
<div class="line"><a name="l00339"></a><span class="lineno">  339</span>&#160;                <span class="comment">/* linktargetfile does not exist (as a file)</span></div>
<div class="line"><a name="l00340"></a><span class="lineno">  340</span>&#160;<span class="comment">                   and we cannot create it. Read-only filesystem?</span></div>
<div class="line"><a name="l00341"></a><span class="lineno">  341</span>&#160;<span class="comment">                   Too many files open in the system?</span></div>
<div class="line"><a name="l00342"></a><span class="lineno">  342</span>&#160;<span class="comment">                   Filesystem full? */</span></div>
<div class="line"><a name="l00343"></a><span class="lineno">  343</span>&#160;                <a class="code" href="xcommon_8c.html#a9c7dab7e21aad8e4f343ba73120e245b">die</a> (<a class="code" href="xcommon_8h.html#aceb08b199631159833ba07c70a15195e">EX_FILEIO</a>, <a class="code" href="nls_8h.html#a8a97de23397d8af6821824c0234d8c3c">_</a>(<span class="stringliteral">&quot;can&#39;t create lock file %s: %s &quot;</span></div>
<div class="line"><a name="l00344"></a><span class="lineno">  344</span>&#160;                                                  <span class="stringliteral">&quot;(use -n flag to override)&quot;</span>),</div>
